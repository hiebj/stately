{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": true,
    "noEmitOnError": true,
    "pretty": true,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "lib": ["dom", "es2017", "esnext.asynciterable"]
  },
  "exclude": ["**/*.spec.*"]
}
